{
  "name": "ng2-playground",
  "version": "0.0.1",
  "author": "Richard Osterloh <richard.osterloh@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "npm run webpack --colors --display-error-details --display-cached",
    "webpack": "webpack",
    "clean": "rimraf node_modules tsd_typings",
    "watch": "webpack --watch",
    "server": "webpack-dev-server --inline --colors --display-error-details --display-cached --port 3000",
    "tsd-update": "tsd reinstall --overwrite",
    "docs": "typedoc --options typedoc.json  src/**/*.ts",
    "postinstall": "npm run tsd-update && tsd install && tsd link",
    "prestart": "npm install",
    "start": "npm run server"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rosterloh/ng2-playground.git"
  },
  "bugs": {
    "url": "https://github.com/rosterloh/ng2-playground/issues"
  },
  "homepage": "https://github.com/rosterloh/ng2-playground",
  "dependencies": {
    "angular2": "^2.0.0-alpha.37",
    "jwt-decode": "^1.4.0",
    "material-design-lite": "1.0.5",
    "reflect-metadata": "^0.1.2",
    "rtts_assert": "^2.0.0-alpha.37",
    "rx": "^4.0.0",
    "whatwg-fetch": "^0.9.0",
    "zone.js": "^0.5.4"
  },
  "devDependencies": {
    "css-loader": "^0.19.0",
    "exports-loader": "^0.6.2",
    "expose-loader": "^0.7.0",
    "file-loader": "^0.8.4",
    "imports-loader": "^0.6.4",
    "json-loader": "^0.5.3",
    "loader-utils": "^0.2.11",
    "object-assign": "^4.0.1",
    "raw-loader": "^0.5.1",
    "style-loader": "^0.12.4",
    "tsd": "^0.6.4",
    "typescript": "^1.6.2",
    "typescript-simple-loader": "^0.3.7",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.12.0",
    "xtend": "^4.0.0"
  }
}
