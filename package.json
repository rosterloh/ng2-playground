{
  "name": "ng2-playground",
  "version": "0.0.1",
  "author": "Richard Osterloh <richard.osterloh@gmail.com>",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf node_modules doc && npm cache clean",
    "watch": "webpack --watch --progress --profile --colors --display-error-details --display-cached",
    "build": "webpack --progress --profile --colors --display-error-details --display-cached",
    "server": "webpack-dev-server --inline --progress --profile --colors --display-error-details --display-cached --port 3000",
    "lint": "tsconfig-lint",
    "docs": "typedoc  --options typedoc.json  src/**/*.ts",
    "start": "npm run server"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rosterloh/ng2-playground.git"
  },
  "bugs": {
    "url": "https://github.com/rosterloh/ng2-playground/issues"
  },
  "homepage": "https://github.com/rosterloh/ng2-playground",
  "dependencies": {
    "core-js": "^1.2.6",
    "rxjs": "5.0.0-alpha.14",
    "angular2": "^2.0.0-alpha.52",
    "jwt-decode": "^1.4.0",
    "material-design-lite": "1.0.6",
    "reflect-metadata": "^0.1.2",
    "whatwg-fetch": "^0.10.1",
    "zone.js": "^0.5.9"
  },
  "devDependencies": {
    "css-loader": "^0.23.0",
    "exports-loader": "^0.6.2",
    "expose-loader": "^0.7.1",
    "file-loader": "^0.8.5",
    "imports-loader": "^0.6.5",
    "json-loader": "^0.5.4",
    "raw-loader": "^0.5.1",
    "style-loader": "^0.13.0",
    "ts-loader": "^0.7.2",
    "tsconfig-lint": "^0.2.0",
    "typedoc": "^0.3.12",
    "typescript": "^1.7.3",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.12.1"
  }
}
