{
  "name": "ng2-playground",
  "version": "0.0.1",
  "author": "Richard Osterloh <richard.osterloh@gmail.com>",
  "homepage": "https://github.com/rosterloh/ng2-playground",
  "license": "MIT",
  "scripts": {
    "clean": "./node_modules/.bin/rimraf node_modules doc typings dist && npm cache clean",
    "clean:ng2": "./node_modules/.bin/rimraf node_modules/angular2/manual_typings",
    "clean:dist": "./node_modules/.bin/rimraf dist",
    "watch": "npm run watch:dev",
    "watch:dev": "./node_modules/.bin/webpack --watch --progress --profile --colors --display-error-details --display-cached",
    "watch:prod": "./node_modules/.bin/webpack --watch --config webpack.prod.config.js --progress --profile --colors --display-error-details --display-cached",
    "build": "npm run build:dev",
    "prebuild:dev": "npm run clean:dist",
    "build:dev": "./node_modules/.bin/webpack --progress --profile --colors --display-error-details --display-cached",
    "prebuild:prod": "npm run clean:dist",
    "build:prod": "./node_modules/.bin/webpack --config webpack.prod.config.js --progress --profile --colors --display-error-details --display-cached",
    "server": "./node_modules/.bin/webpack-dev-server --progress --profile --colors --display-error-details --display-cached",
    "lint": "./node_modules/.bin/tsconfig-lint",
    "docs": "./node_modules/.bin/typedoc --options typedoc.json src",
    "start": "npm run server",
    "postinstall": "npm run clean:ng2 && ./node_modules/.bin/typings install"
  },
  "dependencies": {
    "angular2": "^2.0.0-beta.1",
    "angular2-jwt": "^0.1.6",
    "es6-promise": "^3.0.2",
    "es6-shim": "^0.33.13",
    "es7-reflect-metadata": "^1.4.0",
    "ng2-material": "^0.1.7",
    "rxjs": "5.0.0-beta.0",
    "zone.js": "0.5.10"
  },
  "devDependencies": {
    "compression-webpack-plugin": "^0.2.0",
    "copy-webpack-plugin": "^0.3.3",
    "css-loader": "^0.23.0",
    "exports-loader": "^0.6.2",
    "expose-loader": "^0.7.1",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^1.7.0",
    "http-server": "^0.8.5",
    "imports-loader": "^0.6.5",
    "json-loader": "^0.5.4",
    "node-sass": "^3.4.2",
    "postcss-loader": "^0.8.0",
    "raw-loader": "^0.5.1",
    "reflect-metadata": "0.1.2",
    "rimraf": "^2.5.0",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "ts-helper": "0.0.1",
    "ts-loader": "^0.8.0",
    "tsconfig-lint": "^0.5.0",
    "tslint": "^3.2.2",
    "tslint-loader": "^2.1.0",
    "typedoc": "^0.3.12",
    "typescript": "^1.7.5",
    "typings": "^0.6.2",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.11",
    "webpack-dev-server": "^1.14.1",
    "webpack-md5-hash": "^0.0.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rosterloh/ng2-playground.git"
  },
  "bugs": {
    "url": "https://github.com/rosterloh/ng2-playground/issues"
  },
  "engines": {
    "node": ">= 4.2.1 <= 5",
    "npm": ">= 3"
  }
}
