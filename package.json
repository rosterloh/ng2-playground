{
  "name": "ng2-playground",
  "version": "0.0.1",
  "author": "Richard Osterloh <richard.osterloh@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "npm run build:dev && npm run build:prod",
    "build:dev": "npm run env:dev && npm run webpack --colors --display-error-details --display-cached",
    "build:prod": "npm run env:prod && npm run webpack --colors --display-error-details --display-cached",
    "watch": "webpack --watch",
    "server": "webpack-dev-server --inline --colors --display-error-details --display-cached --port 3000",
    "env:dev": " set NODE_ENV=development",
    "env:prod": "set NODE_ENV=production",
    "webdriver-update": "webdriver-manager update",
    "webdriver-start": "webdriver-manager start",
    "preprotractor": "npm run webdriver-update",
    "protractor": "protractor",
    "postinstall": "tsd reinstall",
    "prestart": "npm install",
    "start": "npm run server",
    "test": "karma start",
    "e2e": "npm run protractor"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rosterloh/ng2-playground.git"
  },
  "bugs": {
    "url": "https://github.com/rosterloh/ng2-playground/issues"
  },
  "homepage": "https://github.com/rosterloh/ng2-playground",
  "dependencies": {
    "angular2": "^2.0.0-alpha.33",
    "jwt-decode": "^1.4.0",
    "material-design-lite": "1.0.2",
    "reflect-metadata": "^0.1.0",
    "rtts_assert": "^2.0.0-alpha.33",
    "rx": "^2.5.3",
    "whatwg-fetch": "^0.9.0",
    "zone.js": "^0.5.2"
  },
  "devDependencies": {
    "css-loader": "^0.15.6",
    "exports-loader": "^0.6.2",
    "expose-loader": "^0.7.0",
    "file-loader": "^0.8.4",
    "imports-loader": "^0.6.4",
    "json-loader": "^0.5.2",
    "karma": "^0.13.3",
    "karma-chrome-launcher": "^0.2.0",
    "karma-coverage": "^0.4.2",
    "karma-jasmine": "^0.3.6",
    "karma-phantomjs-launcher": "^0.2.0",
    "karma-sourcemap-loader": "^0.3.5",
    "karma-webpack": "^1.7.0",
    "loader-utils": "^0.2.11",
    "phantomjs": "^1.9.17",
    "protractor": "^2.1.0",
    "raw-loader": "^0.5.1",
    "style-loader": "^0.12.3",
    "tsd": "^0.6.3",
    "typescript": "^1.5.3",
    "typescript-simple-loader": "^0.3.3",
    "url-loader": "^0.5.6",
    "webpack": "^1.10.5",
    "webpack-dev-server": "^1.10.1",
    "xtend": "^4.0.0"
  }
}
